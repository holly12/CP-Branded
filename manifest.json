{
"name":"CP Branded",
"description":"Rebrand Google.com & Chrome's new tab page",
"version":"0.2",
"author": "Neal Shyam",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["http://www.google.com/*","https://www.google.com/*"],
      "js": ["cp.js"]
    }
  ],
"permissions": ["tabs", "http://www.google.com/*"],
"web_accessible_resources": [ "res/cp.jpg"],
"chrome_url_overrides": { "newtab": "ig.html" },
"icons": { "16": "res/16.jpg",
           "48": "res/48.jpg",
          "128": "res/128.jpg" }



}